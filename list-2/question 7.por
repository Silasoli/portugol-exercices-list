programa {
	funcao inicio() {
		//4 - Ler um valor N e imprimir todos os valores 
		//inteiros entre 1 (inclusive) e N (inclusive). 
		//Considere que o N será sempre maior que ZERO. 
        
        inteiro N
        inteiro X = 1
        escreva("Digite um número para contar de 1 até ele: ")
        leia(N)
        para(X = X; X <= N; X++){
            escreva(X+"\n")
       
        }
	}
}

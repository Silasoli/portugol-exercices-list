programa {
	funcao inicio() {
		//1 - Ler um valor e escrever se é positivo ou negativo (considere o valor zero como positivo). 
	    inteiro val1
		
		escreva("Digite um valor inteiro:\n")
		leia(val1)
		
		se (val1>-1) 
		    escreva(val1+" é um valor positivo!!")
        senao
            escreva(val1+" é um valor negativo!!")

	}
}

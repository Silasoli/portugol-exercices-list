programa {
	funcao inicio() {
		//Escreva um algoritmo para ler um valor (do teclado) 
		//e escrever (na tela) o seu antecessor. 
	    inteiro val1  = 0
	    escreva("Digite um valor inteiro para saber seu antecessor:")
	    leia(val1)
	    escreva("O antecessor do número digitado é:")
	    escreva(val1 - 1)
	}
}
